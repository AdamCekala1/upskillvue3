    "vue-axios": "^3.4.0",
    "vue-router": "^4.0.12",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cypress/vue": "^3.1.1",
    "@pinia/testing": "0.0.11",
    "@types/node": "^16.11.12",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitest/ui": "^0.8.4",
    "cypress": "^9.5.3",
    "happy-dom": "^2.55.0",
